  <data name="GraphPageMainJs" xml:space="preserve">
    <value>var handle = "%handle%";// replace
var connection = null;
var connectionState = 0;
var url  = "ws://" + document.location.hostname + ":%port%";// replace
var titleInitial = "%label%";

%var%

%ctx%

function Authenticate() 
{
    document.getElementById("titleh1").innerHTML = "ImpostorMin - Hang on, connecting to " + titleInitial;
    connection = new WebSocket(url);
	connection.onopen = function(evt) 
	{
		// authenticate insecurely
		var request = 
		{
			Handle: handle,
			Secure: false
		};

		var baseMessage = 
		{
			Stage: 0,
			Data: JSON.stringify(request)
		};

		connection.send(JSON.stringify(baseMessage));
		connection.onmessage = function(evt) 
		{
			Received(evt.data);
		};
	}
}

function Received(str) 
{
	if (connectionState === 0) 
	{
		if (str === "Welcome") 
		{
			console.log("authenticated !");
            document.getElementById("titleh1").innerHTML = "ImpostorMin " + "&lt;b style=\"color: #41BB3F; \"&gt; | " + titleInitial + " |&lt;/b&gt;";
            connectionState = 2;
			Authenticated();
			Plot();
		} 
		else if (str === "HNF") 
		{
			console.log("handle not found.");
			// fail connection
			// how?
			// user is trying to probe the server. Display a message to inform them about that.
		}
	}
	
	else if (connectionState === 1) 
	{
		// not possible
	}
	
	else if (connectionState === 2) 
	{
		Process(str);
	}
}

function Process(str){
	var receivedJson = JSON.parse(str);
	console.log("Rx: "  + str + "\n" + receivedJson);
	%receivers%
}

function Authenticated() {
	// show message
}

window.onload = onload();

function onload() {
	Authenticate();
}

function Plot(){
	%plot%
}</value>
  </data>
  <data name="GraphSiteGrapgJs" xml:space="preserve">
    <value>var %varname% = new Chart(%ctx%, {
	type: 'line',
	data: {
		datasets: [{
			label: '%label%',						// *
			borderColor: '%bcolor%',				// replace settings here
			backgroundColor: '%bgcolor%',			// *
			lineTension: 0,								// *
			borderDash: [8, 4]							// *
	
		}]
	},
	
	options: {
		responsive: true,
		maintainAspectRatio: false,
		scales: {
			yAxes: [{
				ticks: {
					beginAtZero: true,
					precision: 0
				}
			}],
			xAxes: [
			{
				type: 'realtime',
				realtime: {
					delay: %delay%,							// *
					duration: %duration%,					// replace settings here
					onRefresh: function (chart) {
						chart.data.datasets[0].data.push({
							x: Date.now(),
							y: %var%						// replace var dataset here
						});
					}
				}
			}
			]
		}
	},
});</value>
  </data>
  <data name="OneWayConsoleHtmlFull" xml:space="preserve">
    <value>&lt;html&gt;
	&lt;head&gt;
		&lt;meta name="viewport" content="width=device-width, initial-scale=1.0"&gt;
		&lt;meta name="theme-color" content="#ffffff"&gt;
		&lt;link rel="stylesheet" href="water.css"&gt;
		&lt;style&gt;
            html, body {
                font-family: 'Open Sans', 'Lucida Grande', 'Arial', 'Helvetica', 'sans-serif', 'headerFont';
                background-color: #141414;
                max-width: none;
            }

			.container {
				display: inline;
			}

			.container #text {
				width: 80%;
			}

		
            @font-face {
                font-family: headerFont;
                src: url('font/AlfaSlabOne-Regular.ttf') format('truetype');
            }
		&lt;/style&gt;
		
	&lt;/head&gt;
	&lt;body&gt;
		
		
		
		&lt;div class="header" style="display: inline;"&gt;
				&lt;h1 style="display: inline; vertical-align: middle; color:#ff0000; font-family: headerFont;" id="titleh1"&gt;ImpostorMin &lt;b style="color: #41BB3F; "&gt; | %txt% |&lt;/b&gt;&lt;/h1&gt;
				&lt;noscript style="display:inline; color:red;"&gt;ImpostorMin is not compatible with this browser&lt;/noscript&gt;
				&lt;div class="header-right"&gt;
					&lt;!-- TODO: add an animation --&gt;
				&lt;/div&gt;
		&lt;/div&gt;
		
		&lt;div style="height: 100%; width: 100%; display: grid; grid-template-rows:auto auto; grid-template-columns:100% auto;"&gt;
			&lt;div&gt;
				&lt;textarea style="color:%color%; height:150%; width:100%; resize: none;" name="Console" id="Console" placeholder="%mainplaceholder%" readonly&gt;&lt;/textarea&gt;
			&lt;/div&gt;
		&lt;/div&gt;
		
		&lt;script defer&gt; 
			
			var titleInitial = "%txt%";
			var handle = "%handle%";// replace
			var connection = null;
			var connectionState = 0;
			var url  = "ws://" + document.location.hostname + ":%port%";// replace		
			var box = document.getElementById("Console");
			function Authenticate() 
			{
				document.getElementById("titleh1").innerHTML = "ImpostorMin - Hang on, connecting to " + titleInitial;
				connection = new WebSocket(url);
				connection.onopen = function(evt) 
				{
					// authenticate insecurely
					var request = 
					{
						Handle: handle,
						Secure: false
					};
			
					var baseMessage = 
					{
						Stage: 0,
						Data: JSON.stringify(request)
					};
			
					connection.send(JSON.stringify(baseMessage));
					connection.onmessage = function(evt) 
					{
						Received(evt.data);
					};
				}
			}
			
			function Received(str) 
			{
				if (connectionState === 0) 
				{
					if (str === "Welcome") 
					{
						console.log("authenticated !");
						document.getElementById("titleh1").innerHTML = "ImpostorMin " + "&lt;b style=\"color: #41BB3F; \"&gt; | " + titleInitial + " |&lt;/b&gt;";
						connectionState = 2;
						Authenticated();
					} 
					else if (str === "HNF") 
					{
						console.log("handle not found.");
						// fail connection
						// how?
						// user is trying to probe the server. Display a message to inform them about that.
					}
				}
				
				else if (connectionState === 1) 
				{
					// not possible
				}
				
				else if (connectionState === 2) 
				{
					Process(str);
				}
			}
			
			function Authenticated() {
				// show message
			}
			
			
			var firstCell = null;
			function Process(message) {
				var data = JSON.parse(message);
				
				if(data.Clear === true) {
					box.value = "";
					return;
				}
			
				if(data.Set === true)
				{
					box.value = data.Message;
					return;
				}
				else
				{
					box.value += data.Message;
				}
			}
			
			window.onload = onload();
			
			function onload() {
				Authenticate();
			}
			
		
		&lt;/script&gt;
		
	&lt;/body&gt;
&lt;/html&gt;</value>
  </data>
  <data name="SimpleApiPageFull" xml:space="preserve">
    <value>&lt;html&gt;
	&lt;head&gt;
		&lt;style&gt;
            html, body {
                font-family: 'Open Sans', 'Lucida Grande', 'Arial', 'Helvetica', 'sans-serif', 'headerFont';
                background-color: #141414;
                max-width: none;
            }

			.container {
				display: inline;
			}

			.container #text {
				width: 80%;
			}

		
            @font-face {
                font-family: headerFont;
                src: url('font/AlfaSlabOne-Regular.ttf') format('truetype');
            }
		&lt;/style&gt;
		
	&lt;/head&gt;
	&lt;body&gt;
		&lt;div class="header"&gt;
			&lt;div class="header" style="display: inline;"&gt;
				&lt;h1 style="display: inline; vertical-align: middle; color:#ff0000; font-family: headerFont;" id="titleh1"&gt;ImpostorMin&lt;b style="color: #41BB3F; "&gt; | %txt% |&lt;/b&gt;&lt;/h1&gt;
				&lt;noscript style="display:inline; color:red;"&gt;ImpostorMin is not compatible with this browser&lt;/noscript&gt;
			
			&lt;/div&gt;
		&lt;/div&gt;
		&lt;p&gt;&lt;/p&gt;
		&lt;div style="height: 100%; width: 100%; display: grid; grid-template-rows:auto auto; grid-template-columns:50% auto;"&gt;
	
		&lt;div&gt;
			&lt;table style="color: %color%" id="main"&gt;&lt;tr&gt;
			&lt;!-- elements are inserted here automatically by the API client --&gt;
				
				&lt;thead&gt;
					
			
				&lt;/thead&gt;
				
			&lt;/table&gt;
			
			&lt;script defer&gt;
                var titleInitial = "%txt%";
				var handle = "%handle%";// replace
				var connection = null;
				var connectionState = 0;
				var url  = "ws://" + document.location.hostname + ":%port%";// replace
				var parent = document.getElementById("main");
				var table = parent.getElementsByTagName('tbody')[0];
				var createdFirstRow = false;

				function Authenticate() 
				{
                    document.getElementById("titleh1").innerHTML = "ImpostorMin - Hang on, connecting to " + titleInitial;
                    connection = new WebSocket(url);
					connection.onopen = function(evt) 
					{
						// authenticate insecurely
						var request = 
						{
							Handle: handle,
							Secure: false
						};
				
						var baseMessage = 
						{
							Stage: 0,
							Data: JSON.stringify(request)
						};
				
						connection.send(JSON.stringify(baseMessage));
						connection.onmessage = function(evt) 
						{
							Received(evt.data);
						};
					}
				}
				
				function Received(str) 
				{
					if (connectionState === 0) 
					{
						if (str === "Welcome") 
						{
							console.log("authenticated !");
                            document.getElementById("titleh1").innerHTML = "ImpostorMin " + "&lt;b style=\"color: #41BB3F; \"&gt; | " + titleInitial + " |&lt;/b&gt;";
                            connectionState = 2;
							Authenticated();
						} 
						else if (str === "HNF") 
						{
							console.log("handle not found.");
							// fail connection
							// how?
							// user is trying to probe the server. Display a message to inform them about that.
						}
					}
					
					else if (connectionState === 1) 
					{
						// not possible
					}
					
					else if (connectionState === 2) 
					{
						Process(str);
					}
				}
				
				function Authenticated() {
					// show message
				}


                var firstCell = null;
				function Process(message) {
					var data = JSON.parse(message);
					if(data.Clear === true) {
                        createdFirstRow = false;
						while(table.hasChildNodes())
						{
							table.removeChild(table.firstChild);
						}
                        return;
					}

                    var newRow;
                    var newCell;
                    if(data.Set === true)
					{
						if(!createdFirstRow)
						{
							console.log("creating first row for the set operation.");
                            newRow = table.insertRow();
                            newRow.style.backgroundColor = data.Color;
                            firstCell = newRow.insertCell(0);
                            createdFirstRow = true;
						}
						firstCell.innerHTML = data.Message;
                        firstCell.backgroundColor = data.Color;
                    }
					else
					{
                        newRow = table.insertRow();
                        newRow.style.backgroundColor = data.Color;
                        newCell = newRow.insertCell();
                        var newMessage = document.createMessageNode(data.Message);
						newCell.appendChild(newMessage);
					}
				}
				
				window.onload = onload();
				
				function onload() {
					Authenticate();
				}
							
		&lt;/script&gt;
		&lt;/div&gt;
	  &lt;/div&gt;
	&lt;/body&gt;
&lt;/html&gt;</value>
